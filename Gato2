import java.awt.*;
public class Gato2 {
    public static void main(String[] args) {
        Gato gato = new Gato(Color.orange, "grande", "explorador", "Chispita");
        Gato gato1 = new Gato();
        gato1.setNombre("Oreo");
        gato1.setColor(Color.BLACK);
        gato1.setTamaño("Mediano");
        gato1.setTipo("Hogareño");
        System.out.println(gato);
        System.out.println(gato1);
        gato.hogareño();
        gato.gris();
        gato.mediano();

    }
}

